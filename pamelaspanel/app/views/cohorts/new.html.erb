<%= form_for @cohort do |f| %>  
    <%= f.label :name %>  
    <%= f.text_field :name %>  
    <%= f.label :start %>  
    <%= f.date_field :start %>
    <%= f.label :end %>  
    <%= f.date_field :end %>
    <%= f.label :course_id %>  
    <%= f.number_field :course_id %>
    <%= f.submit %>
<% end %>