
<div class="row new">
<%= form_for @user  do |f| %>  
    <%= f.label :first_name %>  
    <%= f.text_field :first_name %>  
    <%= f.label :last_name %>  
    <%= f.text_field :last_name %>
    <%= f.label :email %>  
    <%= f.text_field :email %>
    <%= f.label :password %>  
    <%= f.text_field :password %>
    <%= f.label :age %>  
    <%= f.number_field :age %>
    <%= f.label :degree %>  
    <%= f.select :degree, ['HS','Undergrad' ,'Masters', 'PhD']%>
    <%= f.label :role %>  
    <%= f.select :role, ['Instructor','Student' ,'Administrator']%>
    <%= f.label :avatar %> 
    <%= f.file_field :avatar %>
    <%= f.submit %>
<% end %>

</div>